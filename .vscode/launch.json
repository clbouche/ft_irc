{
	"version": "0.2.0",
	"configurations": [
	  {
		"name": "Build and debug project",
		"type": "cppdbg",
		"request": "launch",
		"program": "${workspaceFolder}/ircserv",
		"args": ["6667"],
		"stopAtEntry": false,
		"cwd": "${workspaceFolder}",
		"environment": [],
		"externalConsole": false,
		"MIMode": "gdb",
		"setupCommands": [
		  {
			"description": "Enable pretty-printing for gdb",
			"text": "-enable-pretty-printing",
			"ignoreFailures": true
		  }
		],
		"preLaunchTask": "C/C++: Build project",
		"miDebuggerPath": "/usr/local/bin/gdb"
	  }
	]
  }
